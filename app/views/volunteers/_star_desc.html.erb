<% if star.board_member == true %>
  <%= image_tag 'blue-star.png', {width: 32 } %>
  <%= star.activity.name %><br>
<% elsif star.activity.name.downcase.include? "blood" %>
  <%= image_tag 'red-star.png', { width: 32 } %>
  <%= star.activity.name %><br>
<% else %>
  <%= image_tag 'star.png', {width: 32 } %>
  <%= star.activity.name %><br>
<% end %>
